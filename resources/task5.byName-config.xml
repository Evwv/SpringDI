<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="toreto" class="com.myroom.task5.Driver" autowire="byName" scope="prototype">
        <property name="price" value="15"/>
    </bean>

    <bean id="car" class="com.myroom.task5.Porshe" scope="prototype"></bean>
    <bean id="bmw" class="com.myroom.task5.Bmw" scope="prototype"></bean>

</beans>